{
  "name": "fantasy-sports-extension",
  "title": "Fantasy Sports Extension",
  "version": "1.0.1",
  "description": "Adds the option to set you roster for the whole week in Yahoo Fantasy Sports.",
  "author": "Patrick Davies <me@patrickdavies.ca> (https://patrickdavies.ca)",
  "repository": {
    "type": "git",
    "url": "https://github.com/bleuarg/fantasy-sports-extension"
  },
  "scripts": {
    "test": "nyc ava",
    "build": "npm-run-all build:js build:gulp",
    "start": "npm-run-all --parallel start:*",
    "build:js": "webpack --config webpack.prod.js",
    "build:gulp": "gulp build",
    "start:js": "webpack-dev-server --config webpack.dev.js",
    "start:gulp": "gulp watch",
    "start:stubs": "stubby -w -d stubs/yahoo.yml -l localhost -s 8899",
    "postbuild": "gulp package"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "left-pad": "^1.2.0",
    "lodash": "^4.17.4",
    "memoizee": "^0.4.11",
    "p-map": "^1.2.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react-modal": "^3.2.1",
    "stubby": "^4.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "ava-fixture": "^0.11.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.6.1",
    "gulp-template": "^5.0.0",
    "gulp-zip": "^4.1.0",
    "image-webpack-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.4",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^4.2.0",
    "sinon-stub-promise": "^4.0.0",
    "style-loader": "^0.19.1",
    "svg-url-loader": "^2.3.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-merge": "^4.1.1"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  }
}
