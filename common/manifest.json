{
  "manifest_version": 2,
  "name": "<%= title %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "content_scripts": [
    {
      "matches": ["https://*.fantasysports.yahoo.com/*"],
      "js": ["bundle.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://*.fantasysports.yahoo.com/*"
  ]
}