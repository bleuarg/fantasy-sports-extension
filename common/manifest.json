{
  "manifest_version": 2,
  "name": "Fantasy Sports Extension",
  "version": "1.0.0",
  "description": "Adds the option to set you roster for the whole season in Yahoo Fantasy Sports.",
  "content_scripts": [
    {
      "matches": ["https://*.fantasysports.yahoo.com/*"],
      "js": ["bundle.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://*.fantasysports.yahoo.com/*"
  ]
}